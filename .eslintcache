      {\n          component: RouteWithSubRoute,\n          icon: '',\n          name: '资料列表',\n          path: '/list',\n          routes: [\n            {\n              component: UserProfile\n            }\n          ]\n        }\n      ]\n    }\n  ]\n};\n","/Users/miku/code/react/widowmaker/src/layout/components/breadcrumb/index.tsx",[],"/Users/miku/code/react/widowmaker/src/layout/components/drawer/index.tsx",[],{"ruleId":"241","replacedBy":"242"},{"ruleId":"243","replacedBy":"244"},{"ruleId":"245","severity":1,"message":"246","line":13,"column":7,"nodeType":"247","messageId":"248","endLine":13,"endColumn":14},{"ruleId":"241","replacedBy":"249"},{"ruleId":"243","replacedBy":"250"},{"ruleId":"245","severity":1,"message":"251","line":1,"column":26,"nodeType":"247","messageId":"248","endLine":1,"endColumn":31},{"ruleId":"252","severity":1,"message":"253","line":24,"column":1,"nodeType":"254","endLine":30,"endColumn":5},{"ruleId":"252","severity":1,"message":"253","line":11,"column":1,"nodeType":"254","endLine":26,"endColumn":3},{"ruleId":"252","severity":1,"message":"255","line":8,"column":1,"nodeType":"254","endLine":37,"endColumn":2},{"ruleId":"252","severity":1,"message":"255","line":5,"column":1,"nodeType":"254","endLine":15,"endColumn":2},{"ruleId":"256","severity":1,"message":"257","line":9,"column":9,"nodeType":"258","endLine":9,"endColumn":115},{"ruleId":"259","severity":1,"message":"260","line":39,"column":5,"nodeType":"261","endLine":39,"endColumn":44},{"ruleId":"259","severity":1,"message":"262","line":39,"column":5,"nodeType":"261","endLine":39,"endColumn":44,"suggestions":"263"},{"ruleId":"245","severity":1,"message":"264","line":5,"column":8,"nodeType":"247","messageId":"248","endLine":5,"endColumn":13},{"ruleId":"252","severity":1,"message":"265","line":11,"column":1,"nodeType":"254","endLine":44,"endColumn":3},{"ruleId":"252","severity":1,"message":"255","line":8,"column":1,"nodeType":"254","endLine":38,"endColumn":2},{"ruleId":"252","severity":1,